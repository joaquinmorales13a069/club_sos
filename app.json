{
  "expo": {
    "name": "club_sos",
    "slug": "club_sos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ICON-clubSOS.png",
    "scheme": "clubsos",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": "./assets/images/ICON-clubSOS.png",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/ICON-clubSOS.png",
        "monochromeImage": "./assets/images/ICON-clubSOS.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/ICON-clubSOS.png",
      "bundler": "metro"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
